{
    "name": "railt/railt",
    "type": "library",
    "description": "The Railt Framework.",
    "license": "MIT",
    "keywords": [
        "graphql",
        "idl",
        "otd",
        "ast",
        "framework",
        "railt",
        "application"
    ],
    "homepage": "http://railt.org",
    "version": "dev-master",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "psr/container": "~1.0",
        "psr/log": "~1.0",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0",
        "illuminate/support": "~5.5",
        "hoa/consistency": "~1.0",
        "hoa/exception": "~1.0",
        "hoa/file": "~1.0",
        "hoa/iterator": "~2.0",
        "hoa/math": "~1.0",
        "hoa/protocol": "~1.0",
        "hoa/regex": "~1.0",
        "hoa/visitor": "~2.0"
    },
    "replace": {
        "railt/http": "self.version",
        "railt/events": "self.version",
        "railt/compiler": "self.version",
        "railt/routing": "self.version",
        "railt/container": "self.version",
        "railt/parser": "self.version",
        "hoa/compiler": "*"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/Railt/",
            "Hoa\\Compiler\\": "src/Hoa/Parser/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.1",
        "monolog/monolog": "~1.9",
        "symfony/finder": "~3.0",
        "cache/filesystem-adapter": "~1.0",
        "railt/webonyx-adapter": "self.version",
        "railt/youshido-adapter": "self.version"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\": "tests"
        }
    },
    "suggest": {
        "railt/webonyx-adapter": "Webonyx GraphQL Adapter for Railt Framework (~self.version).",
        "railt/youshido-adapter": "Youshido GraphQL Adapter for Railt Framework (~self.version).",
        "psr/cache-implementation": "PSR-6 Cache Implementation",
        "psr/simple-cache-implementation": "PSR-16 Cache Implementation"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
